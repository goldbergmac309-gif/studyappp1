{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRunner": "jest-circus/runner",
  "reporters": ["default"],
  "testTimeout": 60000,
  "globals": {
    "ts-jest": {
      "isolatedModules": true
    }
  },
  "runInBand": true
}
